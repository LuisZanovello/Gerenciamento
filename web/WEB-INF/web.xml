        <url-pattern>/PesquisaClienteController</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PesquisaIrregularidadeController</servlet-name>
        <servlet-class>controller.PesquisaIrregularidadeController</servlet-class>
    </servlet>
    <servlet-mapping>
