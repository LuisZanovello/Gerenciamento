           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="4.0">

        <servlet>
            <servlet-name>PesquisaAdminController</servlet-name>
            <servlet-class>controller.PesquisaAdminController</servlet-class>
